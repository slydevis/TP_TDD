package Roman;

public class Roman {

	public int decode(String str)
	{
		int Res = 0;
		for(int i=0; i < str.length(); ++i)
		{
			switch(str.charAt(i))
			{
				case 'I':
					Res++;
					break;
				case 'V':
					if(str.charAt(i - 1) == 'I') Res--;
					Res+=5;
					break;
			}
		}
		return Res;
	}
}
